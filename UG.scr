         //var I98 = haveRef(ioIZ, "SF", "I98", "MZ", n);
         //while (I98 >= 0)
         //{
         //   var ioMZ = SC.ReadIO("MZ", I98, -1, "MA;");
         //   if (ioMZ.GetPropByPath("MA").Contains("", "294", false))
