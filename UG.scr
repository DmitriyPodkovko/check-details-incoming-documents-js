               Warnings.Add(UZ+ioUZ.SNRV+space+mes_PZ+space+dis+space+mes_U8+space+_S0012_87);
         if (getbyFirstThirdValue(ioUZ, "PZ", S1000_88_) &&                     //UGЖPZUGЖU802
