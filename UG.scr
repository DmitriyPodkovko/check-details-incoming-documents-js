               Errors.Add(UZ+ioUZ.SNRV+space+mes_PZ+space+dis+space+mes_U8+space+_S0012_3);
         if (getbyThirdValue(ioUZ, "PZ", S1000_4_) &&                           //КСЖPZКСЖU804
